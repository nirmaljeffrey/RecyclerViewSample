<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.nirmal.jeffrey.recyclerviewsample">
  <uses-permission android:name="android.permission.INTERNET" />
  <application
    android:name=".MyApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name=".userinterface.activity.PagedListAdapterActivity"
      android:parentActivityName=".userinterface.activity.MainActivity" />
    <activity
      android:name=".userinterface.activity.ListAdapterActivity"
      android:parentActivityName=".userinterface.activity.MainActivity" />
    <activity android:name=".userinterface.activity.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name=".userinterface.activity.RecyclerViewAdapterActivity"
      android:parentActivityName=".userinterface.activity.MainActivity" />
  </application>

</manifest>